<?xml version="1.0" ?>
<telemetry xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-um-telemetry-model-driven-cfg">
  <model-driven>
    <destination-groups>
      <destination-group>
        <destination-group-id>PIPELINE</destination-group-id>
        <address-family>
          <ipv4>
            <address>
              <address>198.18.1.127</address>
              <port-number>57400</port-number>
              <encoding>
                <self-describing-gpb/>
              </encoding>
              <protocol>
                <grpc>
                  <no-tls/>
                </grpc>
              </protocol>
            </address>
          </ipv4>
        </address-family>
      </destination-group>
    </destination-groups>
    <sensor-groups>
      <sensor-group>
        <sensor-group-id>BGP-VPN-SENSORS</sensor-group-id>
        <sensor-paths>
          <sensor-path>
            <sensor-path-id>Cisco-IOS-XR-telemetry-model-driven-oper:telemetry-model-driven/subscriptions/subscription</sensor-path-id>
          </sensor-path>
          <sensor-path>
            <sensor-path-id>Cisco-IOS-XR-ipv4-bgp-oper:bgp/instances/instance/instance-active/default-vrf/neighbors/neighbor</sensor-path-id>
          </sensor-path>
          <sensor-path>
            <sensor-path-id>Cisco-IOS-XR-ip-rib-ipv4-oper:rib/vrfs/vrf/afs/af/safs/saf/ip-rib-route-table-names/ip-rib-route-table-name/routes/route</sensor-path-id>
          </sensor-path>
        </sensor-paths>
      </sensor-group>
    </sensor-groups>
    <subscriptions>
      <subscription>
        <subscription-id>10</subscription-id>
        <sensor-groups>
          <sensor-group>
            <sensor-group-id>BGP-VPN-SENSORS</sensor-group-id>
            <sample-interval>5000</sample-interval>
          </sensor-group>
        </sensor-groups>
        <destinations>
          <destination>
            <destination-id>PIPELINE</destination-id>
          </destination>
        </destinations>
      </subscription>
    </subscriptions>
  </model-driven>
</telemetry>
